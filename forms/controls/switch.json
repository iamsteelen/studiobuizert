        },
        "padding": {
          "type": "dimension",
          "value": "{density.interaction.space.none}"
        },
        "focus": {
          "border-radius": {
            "type": "dimension",
            "value": "{form.control.switch.border-radius} + {density.interaction.focus.offset}"
          }
        },
        "height": {
          "type": "dimension",
          "value": "{density.interaction.control.default.size}"
        },
        "thumb": {
          "size": {
            "type": "dimension",
            "value": "{density.interaction.control.default.size} - {brand.dimensions.control.thumb-offset} - {brand.dimensions.control.thumb-offset}"
          },
          "icon": {
            "size": {
              "type": "dimension",
              "value": "{form.control.switch.thumb.size} - {brand.dimensions.control.thumb-offset} - {brand.dimensions.control.thumb-offset}"
            }
          }
        },
        "width": {
          "type": "dimension",
          "value": "{density.interaction.control.default.size} * 2"
