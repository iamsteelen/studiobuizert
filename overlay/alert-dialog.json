{
  "overlay": {
    "alert-dialog": {
      "overlay": {
        "background-color": {
          "value": "{mode.color.overlay.skrim}",
          "type": "color"
        },
        "padding-horizontal": {
          "type": "dimension",
          "value": "{density.dimensions.space.9}"
        },
        "padding-vertical": {
          "type": "dimension",
          "value": "{density.dimensions.space.9}"
        }
      },
      "content": {
        "background-color": {
          "type": "color",
          "value": "{mode.color.neutral.container.minimal}"
        },
        "border-color": {
          "type": "color",
          "value": "{mode.color.neutral.container.transparent}"
        },
        "border": {
          "type": "border",
          "value": {
            "color": "{overlay.alert-dialog.content.border-color}",
            "width": "{overlay.alert-dialog.content.border-width}",
            "style": "solid"
          }
        },
        "box-shadow": {
          "type": "boxShadow",
          "value": "{surface.elevation.panels.large-overlay.default}"
        },
        "border-width": {
          "type": "dimension",
          "value": "{density.interaction.border-width.default}"
        },
        "border-radius": {
          "type": "dimension",
          "value": "{density.interaction.radius.lg}"
        },
        "padding-top": {
          "type": "dimension",
          "value": "{density.interaction.space.6}"
        },
        "padding-bottom": {
          "type": "dimension",
          "value": "{overlay.alert-dialog.content.padding-horizontal}"
        },
        "padding-horizontal": {
          "type": "dimension",
          "value": "{density.interaction.space.7}"
        },
        "gap": {
          "type": "dimension",
          "value": "{density.interaction.space.5}"
        },
        "width-max": {
          "value": "{text-style.line-length-max.body.md} + {overlay.alert-dialog.content.padding-horizontal} + {overlay.alert-dialog.content.padding-horizontal}",
          "type": "dimension"
        },
        "actions": {
          "wrapper": {
            "padding-horizontal": {
              "type": "dimension",
              "value": "{density.interaction.space.none}"
            },
            "padding-vertical": {
              "type": "dimension",
              "value": "{density.interaction.space.none}"
            },
            "gap": {
              "value": "{density.interaction.space.7}",
              "type": "dimension"
            }
          }
        },
        "title": {
          "text-color": {
            "value": "{mode.color.neutral.foreground.quiet}",
            "type": "color"
          }
        },
        "description": {
          "text-color": {
            "value": "{mode.color.neutral.foreground.clear}",
            "type": "color"
          }
        },
        "destructive-actions": {
          "wrapper": {
            "gap": {
              "value": "AUTO",
              "type": "spacing"
            }
          }
        },
        "link": {
          "padding-bottom": {
            "value": "{density.interaction.space.5}",
            "type": "dimension"
          },
          "padding-top": {
            "value": "{density.interaction.space.none}",
            "type": "dimension"
          },
          "padding-horizontal": {
            "value": "{density.interaction.space.none}",
            "type": "dimension"
          },
          "gap": {
            "value": "{density.interaction.space.5}",
            "type": "dimension"
          }
        }
      }
    }
  }
}